    <script type="text/javascript">
        function youtube_popup(elm) {
            var dv1 = document.createElement('div');
            dv1.id = 'dv_youtube_mainblock';
            dv1.style.position = 'absolute';
            dv1.style.left = '0px';
            dv1.style.top = (document.body.scrollTop + document.documentElement.scrollTop) + 'px';
            dv1.style.width = '100%';
            dv1.style.height = '100%';
            dv1.style.cursor = 'pointer';
            dv1.style.opacity = 0;
            dv1.style.background = 'rgba(0,0,0,0.7)';
            dv1.style.zIndex = 1000;
            dv1.style.transition = 'all 400ms linear';
            dv1.onclick = function() {
                document.body.removeChild(this);
                document.body.style.overflow = 'auto';
            }
            var dv2 = document.createElement('div');
            dv2.style.position = 'absolute';
            dv2.style.borderRadius = '10px';
            dv2.style.top = '50%';
            dv2.style.left = '50%';
            dv2.style.width = '670px';
            dv2.style.height = '390px';
            dv2.style.marginLeft = '-335px';
            dv2.style.marginTop = '-195px';
            dv2.style.background = '#FFF';
            dv1.appendChild(dv2);

            var img1 = document.createElement('img');
            img1.src = '/tr/fancybox/fancy_close.png';
            img1.style.position = 'absolute';
            img1.style.left = '655px';
            img1.style.top = '-15px';
            dv2.appendChild(img1);

            var if1 = document.createElement('iframe');
            if1.setAttribute('width', '640');
            if1.setAttribute('height', '360');
            if1.setAttribute('frameborder', '0');
            if1.setAttribute('allowfullscreen', true);
            if1.setAttribute('src', elm.getAttribute('rel'));
            if1.style.margin = '15px';
            dv2.appendChild(if1);

            document.body.style.overflow = 'hidden';
            document.body.appendChild(dv1);
            setTimeout(function() {
                document.getElementById('dv_youtube_mainblock').style.opacity = 1;
            }, 300);
        }

        function sisoPopUp() {
            document.body.style.overflow = 'hidden';
            var w = myWidth();
            var h = myHeight();
            var x = 650;
            var y = 650;
            var dv = document.createElement('div');
            dv.style.position = 'fixed';
            dv.style.zIndex = 10000;
            dv.style.width = '100%';
            dv.style.height = '100%';
            dv.style.top = '0px';
            dv.style.left = '0px';
            dv.style.background = 'rgba(0,0,0,0.7)';
            dv.style.cursor = 'pointer';
            dv.style.transition = 'all 1000ms',
                dv.title = 'Kapatmak için tıklayınız';
            dv.onclick = function() {
                sisoClosePopUp(this);
            }

            var img = document.createElement('img');
            img.style.position = 'relative';

             img.src = 'img/30zafer2025.png';
            img.style.width = x + 'px';
            img.style.height = y + 'px';
            img.style.top = '50%';
            img.style.left = '50%';
            img.style.marginLeft = (-1 * parseInt(x / 2)) + 'px';
            img.style.marginTop = (-1 * parseInt(y / 2)) + 'px';
            img.title = 'Kapatmak için tıklayınız';
            dv.appendChild(img);
            document.body.insertBefore(dv, document.body.childNodes[0]);
        }

        function sisoClosePopUp(elm) {
            elm.style.opacity = 0;
            setTimeout(function() {
                elm.parentNode.removeChild(elm);
                document.body.style.overflow = 'auto';
            }, 1500);
        }

        function myWidth() {
            if (typeof(window.innerWidth) == 'number') {
                return window.innerWidth;
            } else if (document.documentElement && (document.documentElement.clientWidth ||
                    document.documentElement.clientHeight)) {
                return document.documentElement.clientWidth;
            } else if (document.body && (document.body.clientWidth || document.body.clientHeight)) {
                return document.body.clientWidth;
            }
        }

        function myHeight() {
            if (typeof(window.innerWidth) == 'number') {
                return window.innerHeight;
            } else if (document.documentElement && (document.documentElement.clientWidth ||
                    document.documentElement.clientHeight)) {
                return document.documentElement.clientHeight;
            } else if (document.body && (document.body.clientWidth || document.body.clientHeight)) {
                return document.body.clientHeight;
            }
        }
    </script>
    
