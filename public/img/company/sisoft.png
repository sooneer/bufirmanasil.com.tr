<script type="text/javascript">
  // Bu kod, pop-up modalı kontrol etmek için kullanılır
  let popUp = document.getElementById("cookiePopup");

  // Kullanıcı "Kabul Et" düğmesine tıkladığında
  document.getElementById("acceptCookie").addEventListener("click", () => {
    // Tarih nesnesi oluşturulur
    let d = new Date();
    // Geçerlilik süresi 1 dakika eklenir (çerez 1 dakika sonra süresi dolacak)
    d.setMinutes(d.getMinutes() + 7*1440);
    // Çerez oluşturulur: isim = myCookieName, değer = thisIsMyCookie, geçerlilik süresi = 1 dakika
    document.cookie = "myCookieName=thisIsMyCookie; expires=" + d + ";";
    // Popup'ı gizle
    popUp.classList.add("hide");
    popUp.classList.remove("show");
  });

  // Çerezin var olup olmadığı kontrol edilir
  const checkCookie = () => {
    // Çerez okunur ve "=" üzerinden bölünür
    let cookies = document.cookie.split(";");
    let cookieExists = false;

    // Çerezler kontrol edilir
    for (let i = 0; i < cookies.length; i++) {
      let cookie = cookies[i].trim();
      // Çerezin adı "myCookieName" ise var olduğu belirtilir
      if (cookie.indexOf("myCookieName=") === 0) {
        cookieExists = true;
        break;
      }
    }

    // Çerez kontrol edilir ve popup modal gösterilir/gizlenir
    if (cookieExists) {
      // Çerez varsa popup'ı gizle
      popUp.classList.add("hide");
      popUp.classList.remove("show");
    } else {
      // Çerez yoksa popup'ı göster
      popUp.classList.add("show");
      popUp.classList.remove("hide");
    }
  };

  // Sayfa yüklendiğinde çerez varlığı kontrol edilir
  window.onload = () => {
    setTimeout(() => {
      checkCookie();
    }, 2000);
  };
</script>

</body>

